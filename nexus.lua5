-- AUTO TP quand tu ramasses
local function autoTP()
    if savedPositions[1] then
        local char = player.Character
        if char and char:FindFirstChild("HumanoidRootPart") then
            local hrp = char.HumanoidRootPart
            
            -- TP automatique Ã  la base
            for i = 1, 3 do
                hrp.CFrame = CFrame.new(savedPositions[1])
                wait(0.01)
            end
            
            status.Text = "AUTO TP! Brainrot stolen!"
            status.TextColor3 = Color3.new(1, 0, 1)
            
            spawn(function()
                wait(2)
                status.Text = "Ready - Ramasse pour AUTO TP!"
                status.TextColor3 = Color3.new(0.8, 0.8, 0.8)
            end)
        end
    else
        status.Text = "Set base position first!"
        status.TextColor3 = Color3.new(1, 0, 0)
    end
end

-- TP INSTANTANÃ‰
local function instantTP(position)
    if not position then return end
    
    local char = player.Character
    if char and char:FindFirstChild("HumanoidRootPart") then
        local hrp = char.HumanoidRootPart
        
        for i = 1, 3 do
            hrp.CFrame = CFrame.new(position)
            wait(0.01)
        end
        
        status.Text = "Teleported!"
        status.TextColor3 = Color3.new(0, 1, 0)
        
        spawn(function()
            wait(1)
            status.Text = "Ready - Ramasse pour AUTO TP!"
            status.TextColor3 = Color3.new(0.8, 0.8, 0.8)
        end)
    end
end

-- Set Base Position
setBtn1.MouseButton1Click:Connect(function()
    local char = player.Character
    if char and char:FindFirstChild("HumanoidRootPart") then
        savedPositions[1] = char.HumanoidRootPart.Position
        setBtn1.Text = "BASE SAVED!"
        setBtn1.BackgroundColor3 = Color3.new(0, 1, 0)
        
        spawn(function()
            wait(1.5)
            setBtn1.Text = "SET BASE POSITION"
            setBtn1.BackgroundColor3 = Color3.new(0, 0.8, 0)
        end)
    end
end)

-- Test TP
tpBtn1.MouseButton1Click:Connect(function()
