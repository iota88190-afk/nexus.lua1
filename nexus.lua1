-- Fonction TP
local function teleportTo(position)
    local char = player.Character
    if char and char:FindFirstChild("HumanoidRootPart") and position then
        char.HumanoidRootPart.CFrame = CFrame.new(position)
        print("T√©l√©port√© √†:", position)
    end
end

-- Set Position 1
setBtn1.MouseButton1Click:Connect(function()
    local char = player.Character
    if char and char:FindFirstChild("HumanoidRootPart") then
        savedPositions[1] = char.HumanoidRootPart.Position
        setBtn1.Text = "‚úÖ POSITION 1 SAVED"
        setBtn1.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
        
        spawn(function()
            wait(2)
            setBtn1.Text = "üíæ SET POSITION 1"
            setBtn1.BackgroundColor3 = Color3.fromRGB(0, 200, 0)
        end)
    end
end)

