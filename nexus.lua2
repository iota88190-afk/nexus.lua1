-- Fonction TP
local function teleportTo(position)
    local char = player.Character
    if char and char:FindFirstChild("HumanoidRootPart") and position then
        char.HumanoidRootPart.CFrame = CFrame.new(position)
        status.Text = "Teleported!"
        status.TextColor3 = Color3.new(0, 1, 0)
        wait(1)
        status.Text = "Ready"
        status.TextColor3 = Color3.new(0.8, 0.8, 0.8)
    end
end

-- Set Position 1
setBtn1.MouseButton1Click:Connect(function()
    local char = player.Character
    if char and char:FindFirstChild("HumanoidRootPart") then
        savedPositions[1] = char.HumanoidRootPart.Position
        setBtn1.Text = "POSITION 1 SAVED"
        setBtn1.BackgroundColor3 = Color3.new(0, 1, 0)
        status.Text = "Position 1 saved!"
        
        spawn(function()
            wait(2)
            setBtn1.Text = "SET POSITION 1"
            setBtn1.BackgroundColor3 = Color3.new(0, 0.8, 0)
            status.Text = "Ready"
        end)
    end
end)

-- TP Position 1
tpBtn1.MouseButton1Click:Connect(function()
    if savedPositions[1] then
        teleportTo(savedPositions[1])
    else
        status.Text = "No position 1 saved!"
        status.TextColor3 = Color3.new(1, 0, 0)
        wait(2)
        status.Text = "Ready"
        status.TextColor3 = Color3.new(0.8, 0.8, 0.8)
    end
end)

